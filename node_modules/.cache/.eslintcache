avoriteShows: 'Will & Grace, Rick and Morty, Hawaii Five-0',\n        status: 'Taking the dog for a walk!',\n        image: profileMan1\n      },\n      {\n        id: generateId(),\n        isOnline: true,\n        firstName: 'Nick',\n        lastName: 'Thompson',\n        userName: 'guccimaner',\n        password: 'guccifan1',\n        interests: 'rap,gym,women',\n        hobbies: 'shopping,driving fast cars,being a chad',\n        favoriteShows: 'Nitro Circus, Jackass, Ridiculousness',\n        status: 'Watching the X-Games',\n        image: profileMan2\n      },\n      {\n        id: generateId(),\n        isOnline: false,\n        firstName: 'Florence',\n        lastName: 'Smith',\n        userName: 'fancypants92',\n        password: 'louisvutton1',\n        interests: 'money,luxury,fine dining',\n        hobbies: 'watch fitting,suit tailoring,arguing over the pronunciation of escargot',\n        favoriteShows: 'Luxury Ads,Cribs,Sex and the City',\n        status: 'Shopping for a Rolex!',\n        image: profileMan3\n      },\n      {\n        id: generateId(),\n        isOnline: true,\n        firstName: 'Kelly',\n        lastName: 'Roe',\n        userName: 'kelly92',\n        password: 'raptorsfan1',\n        interests: 'sports,shopping,travel',\n        hobbies: 'going on cruises,hiking,learning new cultures',\n        favoriteShows: 'Parts Unknown,SportsCenter,Mad Money',\n        status: 'Hoping to get this job promotion!',\n        image: profileWoman\n      }\n    ]\n  })\n  const [data, setData] = React.useState(null);\n//  working on express api calls\n  React.useEffect(() => {\n    fetch(\"/api\")\n      .then((res) => res.json())\n      .then((data) => setData(data.message));\n  }, []);\n\n  console.log({data});\n\n  console.log(usersState)\n\n  const [activeUser, activeUserState] = useState({})\n\n  function isActiveUser (userRequested) {\n    // userRequested= {userName:'soccerfan1',password:'password1'}\n    console.log(userRequested)\n    usersState.userList.filter(user => {\n      if (userRequested.userName === user.userName && userRequested.password === user.password) {\n        user.isOnline = 'true'\n        activeUserState(user)\n      }\n    })\n  }\n\n  // have this be called when a user opens up profile page and when user completes sign in page - hard code for testing\n\n  console.log({ activeUser })\n\n  return (\n    <div className='App'>\n      <NavigatonBar userId={activeUser.id} userName={activeUser.firstName + ' ' + activeUser.lastName} />\n      {/* we pass activeUser id to our nav bar so it can change url dynamically */}\n\n      <Routes>\n        <Route\n          path={activeUser === undefined ? '/sign-in' : '/profile/' + activeUser.id}\n          element={<UserProfile user={activeUser} userList={usersState.userList} />}\n        />\n        <Route\n          path='/sign-in'\n          element={<UserLoginPage activeUserCheck={isActiveUser} activeUser={activeUser} />}\n        />\n        <Route\n          path='/'\n          element={<LandingPage />}\n        />\n      </Routes>\n\n    </div>\n  )\n}\n\nexport default App\n","/media/ryan/FIle Storage/coding_projects/social-media-app/src/Components/SplashPage.js",["52","53","54"],[],"import React from 'react'\nimport { Card, Col, Container, Row, Stack } from 'react-bootstrap'\nimport ReactDOM from 'react-dom'\nimport splash1 from '../images/splash-1.svg'\nimport splash2 from '../images/splash-2.svg'\nimport splash3 from '../images/splash-3.svg'\nimport githublogo from '../images/github-logo.jpeg'\nimport landingPageImg from '../images/landing-page.jpg'\n\nconst LandingPage = (props) => {\n  return (\n    <div className='splash-page'>\n      <Container fluid className='splash-big-container'>\n        <Row>\n          <Col>\n            <img src={landingPageImg} className='landing-image' alt='multiple cubes with social media logos' />\n          </Col>\n        </Row>\n      </Container>\n\n      <Container fluid className='splash-page-container'>\n        <Row>\n          <Col>\n            <Card bg='dark' border='primary' className='secondary-card'>\n              <Card.Title>React App</Card.Title>\n              <Card.Img variant='top' src={splash1} className='card-images' alt='pc with code open' />\n              <Card.Subtitle>React + Bootstrap</Card.Subtitle>\n              <Card.Body>\n                Welcome to my React Social Media App. This is a mock website soley made to test my own skills. You can make an account but it will not be saved after you leave. Next step is to learn MongoDB and Docker to help maintain a real database.\n              </Card.Body>\n            </Card>\n          </Col>\n          <Col>\n            <Card bg='dark' border='info' className='secondary-card'>\n              <Card.Title>Front End Technologies</Card.Title>\n              <Card.Img variant='top' src={splash2} className='card-images' alt='phone showing app development' />\n              <Card.Subtitle>Reusable Components</Card.Subtitle>\n              <Card.Body>\n                Built with React,React-Bootstrap,React-Router-Dom,Formik and more.\n              </Card.Body>\n            </Card>\n          </Col>\n          <Col>\n            <Card bg='dark' border='danger' className='secondary-card'>\n              <Card.Title>Web Dev Simplified</Card.Title>\n              <Card.Img variant='top' src={splash3} className='card-images' alt='multiple pc displays for programming' />\n              <Card.Subtitle>Learning Frameworks</Card.Subtitle>\n              <Card.Body>\n                Testing new technologies.\n              </Card.Body>\n            </Card>\n          </Col>\n          <Col>\n            <Card bg='dark' border='warning' className='secondary-card'>\n              <Card.Title>Check out my Github!</Card.Title>\n              <Card.Img variant='top' src={githublogo} className='card-images' alt='GitHub logo' />\n              <Card.Subtitle>More Projects</Card.Subtitle>\n              <Card.Body>\n                <a href='https://github.com/RyanZmu' target='_blank' rel='noreferrer'>Github Repo!</a>\n              </Card.Body>\n            </Card>\n          </Col>\n        </Row>\n      </Container>\n    </div>\n  )\n}\n\nexport default LandingPage\n","/media/ryan/FIle Storage/coding_projects/social-media-app/src/Components/LoginPage.js",["55","56","57"],[],"import { Formik, Form, Field } from 'formik'\nimport React from 'react'\nimport { Container, Row, Button, Card } from 'react-bootstrap'\n\nconst UserLoginPage = (props) => {\n\n  return (\n    <div className='login-page'>\n      <Container className='login-container'>\n        <Card bg='dark'>\n          <Formik\n            enableReinitialize={true}\n            displayName='login-formik'\n            initialValues={{\n              userName: '',\n              password: ''\n            }}\n            onSubmit={(event) => {\n              props.activeUserCheck({ userName: event.userName, password: event.password })\n              \n            }}\n          >\n            <Form className='user-form'>\n              <h1>Sign In</h1>\n              <p />\n              <label htmlFor='userName'>User Name</label>\n              <Field id='userNameField' name='userName' placeholder='Enter User Name' required />\n\n              <label htmlFor='password'>Password</label>\n              <Field id='passwordField' name='password' placeholder='Enter Password' required />\n\n              <Button type='submit' variant={props.activeUser.firstName === undefined ? 'primary' : 'success'}>Sign In</Button> \n              {/* in the future make this redirect user to their profile on submit */}\n            </Form>\n          </Formik>\n        </Card>\n      </Container>\n\n      <Container>\n        <Row>\n          <p>\n            <br />\n            Here are some logins to try out different profiles! In the future you will be able to make your own profile. Open developer console to see user objects and then check out their profile! Once the button turns green, click on the profile link to see the user. Will redirect in future builds.\n            <br /><br />\n            username: soccerfan1 password: password1\n            <br />\n            username: kelly92 password: raptorsfan1\n            <br />\n            username:fancypants92 password: louisvutton1\n          </p>\n        </Row>\n      </Container>\n\n    </div>\n  )\n}\n\nexport default UserLoginPage\n","/media/ryan/FIle Storage/coding_projects/social-media-app/src/Components/Navigation.js",["58","59","60","61","62"],[],"import React from 'react'\nimport ReactDOM from 'react-dom'\nimport { Link } from 'react-router-dom'\nimport { Nav, Navbar, NavDropdown } from 'react-bootstrap'\nimport logo from '../images/logo.svg'\n\nconst NavigatonBar = (props) => {\n  return (\n    <div className='main-nav-container'>\n      <Navbar bg='dark' variant='dark' className='nav-bar-container' sticky='top'>\n        <Navbar.Brand as={Link} to='/'>DevelopBook <img src={logo} style={{ width: '5vw', height: '5vh' }} /> </Navbar.Brand>\n        <Nav className='nav-bar-navigation'>\n          <Nav.Link as={Link} to={props.userId === undefined ? '/sign-in' : '/profile/' + props.userId}>Profile</Nav.Link>\n          <Nav.Link as={Link} to='/friends'>Friends</Nav.Link>\n          <Nav.Link as={Link} to='/find-users'>Find Users</Nav.Link>\n        </Nav>\n\n        <Nav bg='dark' variant='dark' className='nav-bar-sign-in'>\n          <NavDropdown menuVariant='dark' title='Sign In/Sign Up'>\n            <NavDropdown.Item as={Link} to='/sign-in'>Sign In</NavDropdown.Item>\n            <NavDropdown.Item as={Link} to='/sign-up'>Sign Up</NavDropdown.Item>\n          </NavDropdown>\n        </Nav>\n\n        <Nav className='nav-bar-sign-out'>\n        {props.userId === undefined ? '' : <Nav.Link as={Link} to='/sign-in'>Logged In As:{props.userName} - Sign Out</Nav.Link>}\n        </Nav>\n        {/* this conditonal is saying if the user name is undefined (so no name is given yet) that the navbar will not change. However if the username is valid the nav bar will then display a new Link with the option to sign out and also will display the users first and last name. We pass the name from the App component controlling state*/}\n      </Navbar>\n    </div>\n  )\n}\n\nexport default NavigatonBar\n","/media/ryan/FIle Storage/coding_projects/social-media-app/src/Components/UserProfile.js",["63","64","65","66","67","68","69","70","71","72","73","74","75","76","77","78","79","80","81","82","83"],[],"import React from 'react'\nimport { Container, Card, Row, Col } from 'react-bootstrap'\nimport ReactDOM from 'react-dom'\nimport { Link } from 'react-router-dom'\n\nconst UserProfile = (props) => {\n  console.log(props.user)\n\n  return (\n    <div className='profile-page-container'>\n      <Container className='profile-page-top'>\n        <Row>\n          <Col>\n            <Card className='user-profile-card'>\n              <Card.Title>{`${props.user.firstName} ${props.user.lastName}`}</Card.Title>\n              <Card.Img className='user-card-image' src={props.user.image} />\n              <Card.Subtitle>\n                <hr />\n                {props.user.isOnline ? 'Online!' : 'Currently Offline'}\n                <br />\n                <br />\n                Current Status: {props.user.status}\n                <hr />\n              </Card.Subtitle>\n              <Card.Body>\n                My Interests: {props.user.interests}\n                <hr />\n                My Hobbies: <br /> {props.user.hobbies}\n                <hr />\n                My Favorite Shows: <br /> {props.user.favoriteShows}\n              </Card.Body>\n            </Card>\n          </Col>\n\n          <Col>\n            <Card className='user-profile-feed'>\n              <Card.Title>What your Friends are saying!</Card.Title>\n              {/* Stopping point, lets continue the profile page later and keep working on the login to show a success and an error if user enters wrong name */}\n\n              {props.userList.map(user => {\n                if (user !== props.user) {\n                  return (\n                    <Card key={user.id} className='user-feed-card'>\n                    <Link to={'/profile/' + user.id } style={{textDecoration:'none',color:'black'}}>\n                      <Card.Title>{user.firstName} {user.lastName}</Card.Title>\n                      <Card.Img className='user-feed-image' src={user.image} />\n                     </Link>  \n                      <Card.Subtitle><br />{user.isOnline ? 'Online' : 'Currently Offline'}</Card.Subtitle>\n                      <Card.Body>{user.status}</Card.Body>\n                    </Card>\n                  )\n                }\n              })}\n            </Card>\n          </Col>\n        </Row>\n      </Container>\n    </div>\n  )\n}\n\nexport default UserProfile\n",{"ruleId":"84","severity":2,"message":"85","line":5,"column":25,"nodeType":"86","messageId":"87","endLine":5,"endColumn":30},{"ruleId":"84","severity":2,"message":"88","line":5,"column":32,"nodeType":"86","messageId":"87","endLine":5,"endColumn":38},{"ruleId":"84","severity":2,"message":"89","line":5,"column":40,"nodeType":"86","messageId":"87","endLine":5,"endColumn":44},{"ruleId":"84","severity":2,"message":"90","line":1,"column":27,"nodeType":"86","messageId":"87","endLine":1,"endColumn":36},{"ruleId":"84","severity":2,"message":"91","line":2,"column":8,"nodeType":"86","messageId":"87","endLine":2,"endColumn":16},{"ruleId":"84","severity":2,"message":"89","line":4,"column":10,"nodeType":"86","messageId":"87","endLine":4,"endColumn":14},{"ruleId":"84","severity":2,"message":"92","line":5,"column":10,"nodeType":"86","messageId":"87","endLine":5,"endColumn":14},{"ruleId":"84","severity":2,"message":"93","line":5,"column":16,"nodeType":"86","messageId":"87","endLine":5,"endColumn":23},{"ruleId":"84","severity":2,"message":"94","line":5,"column":25,"nodeType":"